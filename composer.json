{
    "name": "splot/weblog-module",
    "description": "WebLog module for Splot Framework.",
    "keywords": ["php", "tools", "utils", "log", "debug"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michał Pałys-Dudek",
            "email": "michal@michaldudek.pl"
        }
    ],
     "require": {
        "php": ">=5.3.3",
        "psr/log": "~1.0",
        "michaldudek/clog": "dev-master",
        "michaldudek/foundation": ">=0.9,<2.0",
        "splot/framework": ">=0.9,<2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.x-dev"
        }
    },
    "minimum-stability": "dev",
    "autoload": {
    	"psr-4": {
            "Splot\\WebLogModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Splot\\WebLogModule\\Tests\\": "tests/"
        }
    }
}
